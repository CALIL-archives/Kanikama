{
  "name": "Kanikama",
  "version": "2.0.0",
  "description": "iBeacon based micro location library",
  "main": "kanikama.js",
  "directories": {
    "doc": "doc"
  },
  "dependencies": {
    "geolib": "^2.0.18"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "coffee-script": "^1.10.0",
    "gulp": "^3.9.0",
    "gulp-codo": "^1.0.3",
    "gulp-coffee": "^2.3.1",
    "gulp-exec": "^2.1.2",
    "gulp-mocha": "^2.1.3",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "should": "^7.1.0"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "mocha -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CALIL/Kanikama.git"
  },
  "author": "CALIL Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CALIL/Kanikama/issues"
  },
  "homepage": "https://github.com/CALIL/Kanikama",
  "config": {
    "blanket": {
      "pattern": "/kanikama.js",
      "data-cover-never": [
        "node_modules",
        "public"
      ],
      "loader": "./node-loaders/coffee-script",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  }
}
